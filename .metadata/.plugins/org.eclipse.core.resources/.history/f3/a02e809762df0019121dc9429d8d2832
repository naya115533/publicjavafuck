import java.util.Scanner;

class Student1{
	String name;
	int mat,eng,kor;
	public Student(String name,int mat,int eng,int kor)
	{
		this.name = name; this.mat = mat; this.eng = eng; this.kor=kor;
	}
	int sum()
	{
		return mat + eng +kor;
	}
	
	float avg()
	{
		return sum()/3;
	}
	char grade()
	{
		if (avg()>=90)
		{
			return 'A';
		}
		else if (avg()>=80)
		{
			return 'B';
		}
		else if (avg()>=70)
		{
			return 'C';
		}
		else if(avg()>=60)
		{
			return 'D';
		}
		else 
		{
			return 'F';
		}
	}
}

public class 점수{
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		Student1 st=new Student1();
		st.name = sc.toString();
		st.mat = sc.nextInt();
		st.eng = sc.nextInt();
		st.kor = sc.nextInt();
		System.out.println("총점은" + st.sum() +"평균은" + st.avg() +"학점은" + st.grade());
		
		sc.close();
		System.out.println("");
		

	}
}


