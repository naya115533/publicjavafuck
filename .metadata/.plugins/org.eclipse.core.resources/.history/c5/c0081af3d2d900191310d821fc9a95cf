
public class leedaehyeon2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int [][] array = new int [3][2];
		array[1][0]=8;
		int [][] array2={{1,2},{3,4},{5,6}};
		
		int i,j;
		for(i=0;i<3;i++){
			System.out.println();
			for(j=0;j<2;j++)
				System.out.print(array2[i][j] + " ");
		}
	}

}
